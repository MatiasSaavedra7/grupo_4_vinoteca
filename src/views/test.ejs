<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Pagina de pruebas</title>
</head>
<body>
    <h1>USUARIOS</h1>
    <ul>
        <% users.forEach(user => { %>
            <li><%= user.name %> <%= user.surname %></li>   
            <span>Email: <%= user.email %></span> <br>
            <% for( let i = 0; i < user.products.length; i++ ) { %>
                <span>Productos comprados: <%= user.products[i].name %> - $ <%= user.products[i].price %></span> <br>
                <span>Descripcion: <%= user.products[i].descripcion %></span>
            <% } %>
        <% }) %>
    </ul>
</body>
</html>